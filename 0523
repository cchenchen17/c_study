int main()
{
	int arr[8] = { 3,452,14,14,14,234,132,321 };
	printf("%d\n", sizeof(arr[8]));
	printf("%d\n", sizeof(arr));
	int size = sizeof(arr) / sizeof(arr[4]);
	printf("%d\n", size);
	return 0;
}
